  <recipee:hasIngredients rdf:resource="#PeanutButter"/>
  <recipee:isClassifiedAs rdf:resource="#MiscStyle"/>
  <recipee:hasTemp rdf:resource="#Room"/>
  <recipee:hasTime rdf:resource="#Quick"/>
  <recipee:hasSize rdf:resource="#Small"/>
  <recipee:isHealthy rdf:resource="#healthy"/>
  <recipee:hasAllergy rdf:resource="#Nut"/>
