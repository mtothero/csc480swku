<!-- Peanut Butter and Jelly Recipe-->
<recipee:recipe rdf:ID="PBJ">
  <recipee:name>Peanut Butter and Jelly Sandwich</recipee:name>
  <recipee:hasIngredients rdf:resource="#Bread"/>
